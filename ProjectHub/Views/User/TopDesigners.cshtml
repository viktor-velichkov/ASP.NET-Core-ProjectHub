@model List<DesignerListViewModel>

<div class="col-12" style="text-decoration:underline; text-align:start;">
    <div class="row">
        <div class="col-6" style="text-align:start">
            Top Deigners
        </div>        
    </div>
</div>
@{
    foreach (var investor in Model)
    {
        <div class="col-12" style="background-color: lightskyblue;
                                      margin-right: 0;
                                      margin-left: 0px;
                                      margin-top: 2.5%;
                                      border-radius: 5px;
                                      padding: 2.5%;">
            <div class="row">
                <div class="col-5">
                    <div class="profile-img">
                        @{
                            if (investor.Image.Length == 0)
                            {
                                <img src="data:image/png;base64," class="img-thumbnail" style="height:150px;" />
                            }
                            else
                            {
                                <img src="data:image/png;base64,@Convert.ToBase64String(investor.Image)" class="js-profile-picture img-thumbnail" style="height:150px;" />
                            }
                        }
                    </div>
                </div>
                <div class="col-7">
                    <div class="row">
                        <div class="small-text-bold">Name : </div>
                        <div class="small-text-normal" style="padding-left:2.5%;">@investor.Name</div>
                    </div>
                    <div class="row">
                        <div class="small-text-bold" style="font-weight: bold">Projects : </div>
                        <div class="small-text-normal" style="padding-left: 2.5%;">@investor.ProjectsCount</div>
                    </div>
                    <div class="row">
                        <div class="small-text-bold" style="font-weight: bold">Recommends : </div>
                        <div class="small-text-normal" style="padding-left: 2.5%;">@investor.Recommendations</div>
                    </div>
                    <div class="row">
                        <div class="small-text-bold" style="font-weight: bold">Disapprovals : </div>
                        <div class="small-text-normal" style="padding-left: 2.5%;">@investor.Disapprovals</div>
                    </div>
                </div>                
            </div>
        </div>
       
    }
}





